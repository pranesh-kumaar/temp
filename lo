import matplotlib.pyplot as plt

# Visualize Mini Batch K-Means clusters
plt.figure(figsize=(12, 6))
plt.subplot(1, 2, 1)
plt.scatter(df['CashHoldings'], df['Liabilities'], c=df['KMeans_Cluster'], cmap='viridis')
plt.title('Mini Batch K-Means Clusters')

# Visualize DBSCAN clusters
plt.subplot(1, 2, 2)
plt.scatter(df['CashHoldings'], df['Liabilities'], c=df['DBSCAN_Cluster'], cmap='viridis')
plt.title('DBSCAN Clusters')

plt.tight_layout()
plt.show()

# Visualize the ensemble clusters and outliers
plt.figure(figsize=(12, 6))
plt.subplot(1, 2, 1)
plt.scatter(df['CashHoldings'], df['Liabilities'], c=df['Cluster_Status'].map({'Legitimate Cluster': 0, 'Outlier': 1}), cmap='viridis')
plt.title('Ensemble Clusters and Outliers')

# Visualize outliers separately
outliers_df = df[df['Cluster_Status'] == 'Outlier']
plt.subplot(1, 2, 2)
plt.scatter(outliers_df['CashHoldings'], outliers_df['Liabilities'], color='red', marker='x')
plt.title('Outliers')

plt.tight_layout()
plt.show()