profile_attributes = transformed_df.groupby('Profile Name')['Attribute'].apply(list).to_dict()

link_data = []
for profile, attributes in profile_attributes.items():
    for other_profile, other_attributes in profile_attributes.items():
        if profile != other_profile and set(attributes) == set(other_attributes):
            link_data.append({'Profile Name': profile, 'Linked Profile': other_profile})
