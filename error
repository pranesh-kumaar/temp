import pandas as pd
import numpy as np

# Load your dataset
data = pd.read_csv('your_dataset.csv')

# Calculate the percentage of missing values for each column
missing_percentage = (data.isnull().sum() / len(data)) * 100

# Select columns with the least missing values
threshold_percentage = 10  # Adjust as needed
columns_to_include = missing_percentage[missing_percentage < threshold_percentage].index.tolist()

print("Columns with less than", threshold_percentage, "% missing values:")
print(columns_to_include)